{
  "realm": "autorepairshop",
  "enabled": true,
  "sslRequired": "none",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "clients": [
    {
      "clientId": "frontend",
      "enabled": true,
      "publicClient": true,
      "redirectUris": ["http://localhost:5173/*", "http://localhost:3000/*"],
      "postLogoutRedirectUris": ["http://localhost:5173/*", "http://localhost:3000/*"],
      "webOrigins": ["http://localhost:5173", "http://localhost:3000"],
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "protocol": "openid-connect",
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    {
      "clientId": "gateway-service",
      "enabled": true,
      "publicClient": false,
      "secret": "gateway-secret-change-in-prod",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false
    },
    {
      "clientId": "svc-tenantadmin",
      "enabled": true,
      "publicClient": false,
      "secret": "svc-tenantadmin-secret",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false
    },
    {
      "clientId": "svc-customer",
      "enabled": true,
      "publicClient": false,
      "secret": "svc-customer-secret",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false
    }
  ],
  "roles": {
    "realm": [
      {"name": "OWNER"},
      {"name": "ADMIN"},
      {"name": "STAFF"},
      {"name": "MECHANIC"},
      {"name": "CUSTOMER"}
    ]
  },
