{
  "realm": "autorepairshop",
  "enabled": true,
  "sslRequired": "none",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "clients": [
    {
      "clientId": "frontend",
      "enabled": true,
      "publicClient": true,
      "redirectUris": ["http://localhost:5173/*", "http://localhost:3000/*"],
      "webOrigins": ["http://localhost:5173", "http://localhost:3000"],
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "protocol": "openid-connect",
      "defaultClientScopes": ["openid", "profile", "email", "tenant-claims"],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    {
      "clientId": "gateway-service",
      "enabled": true,
      "publicClient": false,
      "secret": "gateway-secret-change-in-prod",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false
    },
    {
      "clientId": "svc-tenantadmin",
      "enabled": true,
      "publicClient": false,
      "secret": "svc-tenantadmin-secret",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false
    },
    {
      "clientId": "svc-customer",
      "enabled": true,
      "publicClient": false,
      "secret": "svc-customer-secret",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false
    }
  ],
  "roles": {
    "realm": [
      {"name": "OWNER"},
      {"name": "ADMIN"},
      {"name": "STAFF"},
      {"name": "MECHANIC"},
      {"name": "CUSTOMER"}
    ]
  },
  "users": [
    {
      "username": "owner@demo.com",
      "email": "owner@demo.com",
      "enabled": true,
      "emailVerified": true,
      "credentials": [{"type": "password", "value": "password123"}],
      "realmRoles": ["OWNER"],
      "attributes": {
        "tenant_id": ["00000000-0000-0000-0000-000000000001"],
        "branch_id": ["00000000-0000-0000-0000-000000000010"]
      }
    },
    {
      "username": "admin@demo.com",
      "email": "admin@demo.com",
      "enabled": true,
      "emailVerified": true,
      "credentials": [{"type": "password", "value": "password123"}],
      "realmRoles": ["ADMIN"],
      "attributes": {
        "tenant_id": ["00000000-0000-0000-0000-000000000001"],
        "branch_id": ["00000000-0000-0000-0000-000000000010"]
      }
    },
    {
      "username": "mechanic@demo.com",
      "email": "mechanic@demo.com",
      "enabled": true,
      "emailVerified": true,
      "credentials": [{"type": "password", "value": "password123"}],
      "realmRoles": ["MECHANIC"],
      "attributes": {
        "tenant_id": ["00000000-0000-0000-0000-000000000001"],
        "branch_id": ["00000000-0000-0000-0000-000000000010"]
      }
    }
  ],
  "clientScopes": [
    {
      "name": "openid",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "false"
      }
    },
    {
      "name": "profile",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true"
      },
      "protocolMappers": [
        {
          "name": "username",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "username",
            "claim.name": "preferred_username",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "full name",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-full-name-mapper",
          "config": {
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        }
      ]
    },
    {
      "name": "email",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true"
      },
      "protocolMappers": [
        {
          "name": "email",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "email",
            "claim.name": "email",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "email verified",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "emailVerified",
            "claim.name": "email_verified",
            "jsonType.label": "boolean",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        }
      ]
    },
    {
      "name": "roles",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "false"
      },
      "protocolMappers": [
        {
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "config": {
            "multivalued": "true",
            "claim.name": "realm_access.roles",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        }
      ]
    },
    {
      "name": "web-origins",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "false"
      },
      "protocolMappers": [
        {
          "name": "allowed web origins",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-allowed-origins-mapper",
          "config": {}
        }
      ]
    },
    {
      "name": "tenant-claims",
      "protocol": "openid-connect",
      "protocolMappers": [
        {
          "name": "tenant_id",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "tenant_id",
            "claim.name": "tenant_id",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        },
        {
          "name": "branch_id",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "branch_id",
            "claim.name": "branch_id",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        }
      ]
    }
  ],
  "defaultDefaultClientScopes": ["openid", "profile", "email", "roles", "web-origins", "tenant-claims"]
}
